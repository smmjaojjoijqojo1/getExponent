class Main {
  public static void main(String[] args) {

    // Math.getExponent() with float variable
    float a = 50.8f;
    System.out.println(Math.getExponent(a));  // 5

    // Math.getExponent with double variable
    double b = 89.3d;
    System.out.println(Math.getExponent(b));  // 6

  }
}
